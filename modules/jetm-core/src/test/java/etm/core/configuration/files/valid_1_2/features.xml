<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jetm-config PUBLIC "-// void.fm //DTD JETM Config 1.2//EN" "http://jetm.void.fm/dtd/jetm_config_1_2.dtd">
<jetm-config>
  <features>
    <threshold-buffer threshold="1000"/>
    <persistence>
      <file-backend filename="ser" path="${java.io.tmpdir}"/>
    </persistence>

    <jmx overwrite="true"
         measurementDomain="performance" monitorObjectName="test:service=Foo"/>
  </features>

</jetm-config>